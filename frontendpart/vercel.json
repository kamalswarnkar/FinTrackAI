{
  "functions": {
    "src/main.jsx": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/auth/success",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}